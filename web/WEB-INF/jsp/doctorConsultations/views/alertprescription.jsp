<div class="row">
    <div class="col-md-12">
        <div class="form-group-sm">
            <label for="visit-number" class="control-label">Visit Number:</label>
            <input type="text" value="" id="visit-number" name="visit-number" class="form-control" disabled="disabled"/>
        </div>
        <div class="form-group-sm">
            <label for="reference-number" class="control-label">Reference Number:</label>
            <input type="text" value="" id="reference-number" name="reference-number" class="form-control" disabled="disabled"/>
        </div>
        <div class="form-group-sm">
            <label for="item-name" class="control-label">Item Name:</label>
            <input type="text" id="item-name" name="item-name" value="" class="form-control" disabled="disabled"/>
        </div>
        <div class="form-group-sm">
            <label for="dosage" class="control-label">Dosage:</label>
            <input type="text" id="dosage" name="dosage" value="" class="form-control" disabled="disabled"/>            
        </div> 
        <div class="form-group-sm">
            <label for="dose" class="control-label">Active Dose: </label>
            <input type="text" class="form-control" id="dose" name="dose" value=""  disabled="disabled" />
        </div>
        <div class="form-group-sm">
            <label for="duration" class="control-label">Remaining Time for Prescription to expire :(Days)</label>
            <input type="text" id="duration" name="duration" class="form-control" value=""  disabled="disabled" />
        </div>

        <div class="form-group-sm">
            <label for="date-issued" class="control-label">Date Issued:</label>
            <input type="text" id="date-issued" name="date-issued" class="form-control" value=""  disabled="disabled" />
        </div>
    </div>
</div>